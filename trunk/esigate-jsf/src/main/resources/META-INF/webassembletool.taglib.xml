  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
	<namespace>http://www.esigate.org</namespace>
	<tag>
		<tag-name>includeblock</tag-name>
		<component>
			<component-type>org.esigate.jsf.IncludeBlockComponent</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>includetemplate</tag-name>
		<component>
			<component-type>org.esigate.jsf.IncludeTemplateComponent</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>includeparam</tag-name>
		<component>
			<component-type>org.esigate.jsf.IncludeParamComponent</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>replace</tag-name>
		<component>
			<component-type>org.esigate.jsf.ReplaceComponent</component-type>
		</component>
	</tag>
</facelet-taglib>
